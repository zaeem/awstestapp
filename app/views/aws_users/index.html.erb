<p id="notice"><%= notice %></p>

<h1>Listing Aws Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_users.each do |aws_user| %>
      <tr>
        <td><%= aws_user.first_name %></td>
        <td><%= aws_user.last_name %></td>
        <td><%= aws_user.email %></td>
        <td><%= link_to 'Show', aws_user %></td>
        <td><%= link_to 'Edit', edit_aws_user_path(aws_user) %></td>
        <td><%= link_to 'Destroy', aws_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws user', new_aws_user_path %>
